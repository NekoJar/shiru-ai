generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Peoples {
  id         Int      @id @default(autoincrement())
  createdAt  DateTime @default(now())
  totalsUp   Int      @default(0)
  totalsDown Int      @default(0)
  totals     Json
  down       Int      @default(0)
  up         Int      @default(0)
}
